{
  "name": "petrolshare",
  "version": "1.2.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest --watchAll",
    "build-web": "expo export:web",
    "build-android": "eas build --profile preview --platform android",
    "build": "yarn build-web && yarn deploy",
