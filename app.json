{
  "expo": {
    "name": "PetrolShare",
    "slug": "PetrolShare",
    "version": "1.1.7",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "petrolshare",
    "description": "The web version of the PetrolShare App",
    "userInterfaceStyle": "automatic",
    "plugins": [
      "./withAndroidVerifiedLinksWorkaround",
      "sentry-expo",
      "@react-native-firebase/app"
    ],
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#292B36"
    },
    "backgroundColor": "#292B36",
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/5a6b67c6-532d-402b-ab54-ceeb04084203"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rsnfreud.PetrolShare"
    },
    "android": {
      "googleServicesFile": "",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#292B36"
      },
      "package": "com.rsnfreud.PetrolShare",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "petrolshare",
              "host": "*.freud-online.co.uk"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "extra": {
      "eas": {
        "projectId": "5a6b67c6-532d-402b-ab54-ceeb04084203"
      }
    },
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "sentry",
            "project": "petrolshare",
            "url": "http://freud-online.co.uk:9000/"
          }
        }
      ]
    }
  }
}