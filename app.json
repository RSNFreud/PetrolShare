{
  "expo": {
    "name": "PetrolShare",
    "slug": "PetrolShare",
    "version": "1.1.4",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "petrolshare",
    "description": "The web version of the PetrolShare App",
    "userInterfaceStyle": "automatic",
    "plugins": [
      "./withAndroidVerifiedLinksWorkaround"
    ],
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#001E24"
    },
    "backgroundColor": "#001E24",
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/5a6b67c6-532d-402b-ab54-ceeb04084203"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "googleServicesFile": "./assets/google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#001e24"
      },
      "permissions": [
        "ACCESS_BACKGROUND_LOCATION",
        "ACCESS_FINE_LOCATION",
        "WAKE_LOCK"
      ],
      "package": "com.rsnfreud.PetrolShare",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "*.freud-online.co.uk"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "extra": {
      "eas": {
        "projectId": "5a6b67c6-532d-402b-ab54-ceeb04084203"
      }
    }
  }
}
