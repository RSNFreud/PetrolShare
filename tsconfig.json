{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@components/*": [
        "./components/*"
      ],
      "@icons/*": [
        "./assets/icons/*"
      ],
    },
    "baseUrl": ".",
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js",
    "dist"
  ]
}